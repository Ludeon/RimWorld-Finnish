<?xml version="1.0" encoding="UTF-8"?>
<LanguageData>
  
  <!-- EN: Follow master while the master is drafted. -->
  <FollowDrafted.headerTip>Seuraa isäntää, kun isäntä on värvätty.</FollowDrafted.headerTip>
  
  <!-- EN: Follow master while the master is doing field work (hunting or taming animals). -->
  <FollowFieldwork.headerTip>Seuraa isäntää, kun isäntä tekee kenttätöitä (metsästää tai kesyttää eläimiä).</FollowFieldwork.headerTip>
  
  <!-- EN: Mark animals to be hunted by your hunters. -->
  <Hunt.headerTip>Merkitse eläimet metsätäjien metsästettäviksi.</Hunt.headerTip>
  
  <!-- EN: Release to wild -->
  <ReleaseAnimalToWild.headerTip>Vapauta luontoon.</ReleaseAnimalToWild.headerTip>
  
  <!-- EN: Slaughter -->
  <Slaughter.headerTip>Teurastus</Slaughter.headerTip>
  
  <!-- EN: Tame a wild animal. -->
  <Tame.headerTip>Kesytä villieläin.</Tame.headerTip>
  
</LanguageData>